    "\n",
    "plt.subplot(224)\n",
    "plt.hist(x, 25, histtype='step', color='tab:blue', density=True, linewidth=2, label=\"Original\")\n",
    "plt.hist(ys, 25, histtype='step', color='tab:red', density=True, linewidth=2, label=\"Generated\")\n",
    "plt.legend()"
