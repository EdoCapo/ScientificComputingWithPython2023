    "hist, bin_edges = np.histogram(X, bins=100, density=True)\n",
    "bin_centers = (bin_edges[:-1] + bin_edges[1:]) / 2.\n",
    "plt.step(bin_centers, factor*hist, linewidth=2)\n",
    "\n",
    "plt.axis([-4, 4, 0, 0.4])\n",
    "plt.title(\"Histogram of accepted samples\")"
