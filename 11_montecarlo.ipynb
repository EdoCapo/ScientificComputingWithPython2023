    "plt.text(px + 0.25, 0.2, 'Reject', fontsize=16)\n",
    "plt.text(px + 0.25, 0.01, 'Accept', fontsize=16)\n",
    "plt.axis([-4, 4, 0, 0.4])\n",
    "plt.title(\"Rejection sampling concepts\")\n",
