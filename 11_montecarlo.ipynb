    "N = 10\n",
    "a, b = 0, 1\n",
    "\n",
    "def f(x):\n",
    "    return 4.*math.sqrt(1 - x**2)\n",
    "\n",
    "def mean_value(x_min, x_max, n):\n",
    "    X = np.random.uniform(x_min, x_max, n)\n",
    "    Y = np.array([f(Xi) for Xi in X])\n",
    "    s = np.sum(Y)\n",
    "    integ = (x_max - x_min) / n * s\n",
    "    return integ\n",
